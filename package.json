{
  "name": "vite-storybook-poc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npm run write-tailwind-conf && npm run write-themes && npm run storybook",
